<div class="re-contract-massage" ng-controller="contractmesCtrl" ng-app="contractmes">
	<div class="re-contract-main" ng-repeat = "cm in cmlist">
		<div>
			<p>租期</p>
			<p>{{cm.rent_period_from}}至{{cm.rent_period_to}}</p>
			<p class="re-contStatus">{{cm.payoff_status?'进行中':'已结束'}}</p>
		</div>
		<div>
			<p>租金</p>
			<p>共{{cm.contract_amt}}元</p>
			<p>
				<img src="images/re-right.png"/>
			</p>
		</div>
	</div>
</div>
